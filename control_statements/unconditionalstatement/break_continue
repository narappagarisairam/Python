#break statement is used to break the condition
for i in range(10):
    if i == 5:
        break
    print(i)
#output
0
1
2
3
4
#continue is used to skip the current iteration
for i in range(10):
    if i % 2 == 0:
        continue
    print(i)
#output
1
3
5
7
9
#pass statement
for i in range(5):
    if i == 3:
        pass  # do nothing
    else:
        print(i)
#output
0
1
2
4

